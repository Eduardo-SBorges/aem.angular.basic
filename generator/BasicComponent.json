{
  "project-settings": {
    "code-owner": "Angular App",
    "bundle-path": "../core/src/main/java",
    "test-path": "../core/src/test/java",
    "apps-path": "../ui.apps/src/main/content/jcr_root/apps",
    "component-path": "angularapp",
    "model-interface-pkg": "com.angularapp.core.models",
    "model-impl-pkg": "com.angularapp.core.models.impl"
  },
  "name": "basic-component",
  "title": "Basic Component",
  "group": "Angular App - Content",
  "type": "components",
  "options": {
    "js": false,
    "jstxt": false,
    "css": false,
    "csstxt": false,
    "html": false,
    "html-content": false,
    "slingmodel": true,
    "testclass": false,
    "junit-major-version": 5,
    "content-exporter": true,
    "model-adaptables": [
      "request"
    ],
    "generic-javadoc": false,
    "properties": [
      {
        "field": "text",
        "label": "Meu Texto",
        "type": "textfield",
        "json-expose": true,
        "json-property": "text"
      },
      {
        "field": "checkbox",
        "type": "checkbox",
        "json-expose": true,
        "attributes": {
          "value": "{Boolean}true",
          "text": "Meu Checkbox"
        }
      }
    ]
  }
}